                <bindingRedirect oldVersion="0.0.0.0-1.2.10.0"
                                 newVersion="1.2.10.0"/>
            </dependentAssembly>
        </assemblyBinding>
        <NetFx40_LegacySecurityPolicy enabled="true"/>
    </runtime>
